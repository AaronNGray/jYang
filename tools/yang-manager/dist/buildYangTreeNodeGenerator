#!/bin/bash

cd ../bin
jar cvfm ../dist/YangTreeNodeGenerator.jar ../dist/MANIFEST.MF *.class applet yangtree
cd ../images
jar uf ../dist/YangTreeNodeGenerator.jar icons/*.png
cd ../../parser/parser
jar uf /root/workspace/yang-applet-generator/dist/YangTreeNodeGenerator.jar jyang/jyang.class jyang/parser/*.class jyang/parser/*.properties 


